[user]
	name = Geoff Corey
	email = geoff@geoffcorey.com
[pull]
	rebase = false
[core]
	pager = delta --dark
[delta]
	side-by-side = true
	line-numbers = true
[filter "lfs"]
	clean = git-lfs clean -- %f
	smudge = git-lfs smudge -- %f
	process = git-lfs filter-process
	required = true
[credential "https://github.com"]
	helper = 
	helper = !/usr/bin/gh auth git-credential
[credential "https://gist.github.com"]
	helper = 
	helper = !/usr/bin/gh auth git-credential
[credential]
	helper = store
